local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Kavra's Kingdom Personal Script", "Synapse")

local Tab = Window:NewTab("Receptionist Section")
local Section = Tab:NewSection("Receptionist Dialouges")

-- RECEPTIONISTS SECTION
Section:NewButton("Open Desk Dialouge", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Open Desk!", "All")
end)

Section:NewButton("Greetings Dialogue", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Greetings Visitor! My name is HellHounds and welcome to Kavra's Kingdom!, Would you like to acquire a job today?", "All")
end)

Section:NewButton("Rank Checking Dialouge", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Alright!, Please allow me to check your rank to see if you're suitable for the job, and kindly wait for a little bit.", "All")
end)

Section:NewButton("Rank Checking Dialogue (Reject Visitor)", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Unfortunately, It seems like you're a guest, and you are required to be a recruit to get a job, to acquire the role recruit, please join the group called Kavra's Kingdom and then come back.", "All")
end)

Section:NewButton("Rank Checking Dialogue (Accept Visitor)", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("It seems like you already have the role called recruit, and once again, welcome to Kavra's Kingdom!", "All")
end)

Section:NewButton("Sending to Security Room Dialouge", "...", function()
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Now, I'll proceed to send you to the waiting room, Good luck!!", "All")
end)

